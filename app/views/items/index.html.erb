<div class="container">
  <div class="row">
    <div class="col">
      <% all_items.each do |item| %>
      <%@item = item %>
        <p><%= image_tag item.image_url, width: 200 %></p>
        <p><%= item.price %> €</p>
        <p><a href="<%= item_path(@item.id)%>" class="btn btn-info">Découvrir la photo</a></p><br>
      <% end %>
    </div>
  </div>
</div>
